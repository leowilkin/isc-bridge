# Required
ICS_URL=https://example.com/path/to/calendar.ics
GOOGLE_CALENDAR_ID=primary

# Optional
SYNC_LOOKAHEAD_DAYS=30
SYNC_LOOKBACK_DAYS=1
POLL_SECONDS=900
# Prefix added to created events' summary in Google. Set empty to disable.
SUMMARY_PREFIX=[School]
# If true, only create "busy blocker" events with no titles (visibility remains private)
BUSY_BLOCKERS=false

# Admin Panel Authentication
ADMIN_PASSWORD=changeme
SECRET_KEY=your-random-secret-key-here
